  <!--
    application.css is where you put your styles
  -->
  <link rel="stylesheet" href="/stylesheets/application.css">

  <link rel="stylesheet" href="http://yui.yahooapis.com/pure/0.5.0/pure-min.css">
  <link rel="stylesheet" href="/stylesheets/pure-layout.css">
  <link rel="stylesheet" href="/stylesheets/main.css">
  <link rel="stylesheet" href="/stylesheet/dhtmlxscheduler.css" type="text">

  <link href='http://fonts.googleapis.com/css?family=Shadows+Into+Light|Gloria+Hallelujah|Indie+Flower|Dancing+Script' rel='stylesheet' type='text/css'>

  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
  <script src="/javascript/application.js"></script>
  <script src="/javascript/dhtmlxscheduler.js" type="text/javascript"></script>

   <script src="/javascript/dhtmlxscheduler.js" type="text/javascript"></script>
   <link rel="stylesheet" href="/stylesheets/dhtmlxscheduler.css" type="text/css">
  <link rel="stylesheet" href="/stylesheets/application.css">

  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
  <script src="/javascript/application.js"></script>

  <script type="text/javascript" charset="utf-8">
  function init() {
    
    scheduler.config.xml_date="%Y-%m-%d %H:%i";
    scheduler.config.date_step = "5"
    scheduler.config.details_on_create = true;
    scheduler.config.details_on_dblclick = true;
    scheduler.init('scheduler_here',"","month");
    scheduler.load("./xml/events.xml");
    
  }

    /*this function is used to show notification to PC users and has no connection to scheduler itself*/
    function showNotification(){
        if (navigator.userAgent.match(/iPad/i) == null)
            document.getElementById("notification").style.display="";
    }
</script>

  <title></title>
</head>
<body onload="init();showNotification()">
  <%= yield %>
</body>
</html>
